{
  "name": "@vhc/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:worker\"",
    "dev:api": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/services/worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/services/worker.js",
    "lint": "tsc --noEmit",
    "seed": "tsx src/scripts/seed.ts",
    "seed:template": "tsx src/scripts/seed-template.ts",
    "setup:dms-key": "tsx src/scripts/setup-dms-key.ts",
    "test": "tsx src/tests/health-check.test.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "bullmq": "^5.66.5",
    "dotenv": "^17.2.3",
    "hono": "^4.0.0",
    "ioredis": "^5.9.1",
    "puppeteer": "^24.35.0",
    "resend": "^6.7.0",
    "socket.io": "^4.8.3",
    "twilio": "^5.11.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "concurrently": "^9.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
